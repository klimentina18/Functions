operator = input()
a = int(input())
b = int(input())

def calculator (a,b, operator):
    if operator == "multiply":
        return  a * b
    elif operator == "divide":
        return  a // b
    elif operator == "add":
        return  a + b
    elif operator == "subtract":
        return  a - b

print(calculator(a,b,operator)



