password = input()

is_len = 6 <= len(password) <= 10
is_digit_letters= password.isalnum()
true_digits = len([i for i in password if i.isdigit()]) >= 2

if is_len and is_digit_letters and true_digits:
    print("Password is valid")
else:
    if not is_len:
        print("Password must be between 6 and 10 characters")
    if not is_digit_letters:
        print("Password must consist only of letters and digits")
    if not true_digits:
        print("Password must have at least 2 digits")
        
        





